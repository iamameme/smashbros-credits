{
  "name": "space-game",
  "version": "1.0.0",
  "homepage": "https://iamameme.github.io/smashbros-credits",
  "description": "A small demo that shows how to manage game entities.",
  "keywords": [
    "zustand",
    "physics",
    "game",
    "drei"
  ],
  "main": "src/index.js",
  "dependencies": {
    "@react-three/fiber": "6.0.15",
    "@seregpie/three.text-sprite": "^3.2.0",
    "detect-gpu": "^4.0.28",
    "pg": "^8.7.3",
    "pg-native": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "styled-components": "5.1.0",
    "three": "0.117.0",
    "zustand": "3.4.1"
  },
  "devDependencies": {
    "typescript": "3.3.3"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider PGDATABASE=smashgame PORT=3007 react-scripts start",
    "build": "PUBLIC_URL=/smashbros-credits NODE_OPTIONS=--openssl-legacy-provider react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "PUBLIC_URL=/smashbros-credits NODE_OPTIONS=--openssl-legacy-provider npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
